    <EmbeddedResource Include="Images\honey2.jpg" />
    <EmbeddedResource Include="Images\honey3.jpg" />
    <EmbeddedResource Include="Images\honeyImage.png" />
    <EmbeddedResource Include="Images\mainlogo.png" />
  </ItemGroup>

  <ItemGroup>
