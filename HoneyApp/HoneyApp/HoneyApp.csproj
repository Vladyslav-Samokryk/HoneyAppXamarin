<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\iconsfont.ttf" />
    <None Remove="Images\honey1.jpg" />
    <None Remove="Images\honey2.jpg" />
    <None Remove="Images\honey3.jpg" />
    <None Remove="Images\honeyImage.png" />
    <None Remove="Images\mainlogo.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Fonts\iconsfont.ttf" />
